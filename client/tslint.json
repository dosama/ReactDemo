{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "jsx-no-lambda": false,
    "interface-name": false,
    "ordered-imports": false,
    "comment-format": false,
    "no-debugger": false,
    "no-empty": false,
    "interface-over-type-literal": false,
    "no-console": false
  },
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
  }
}
